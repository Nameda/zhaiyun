<!--home.wxml-->
<view class="container">
  <!-- 轮播图 -->
  <swiper indicator-dots="true" autoplay="true"  interval="3500" duration="500" class="banner-box" >
    <block wx:for="{{imgUrls}}" wx:key="{{index}}">
      <swiper-item>
        <image src="{{item}}" class="slider-image" width="750rpx" height="400rpx"/>
      </swiper-item>
    </block>
  </swiper>
  
</view>
